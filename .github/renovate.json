{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>jbanghub/.github"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        ".?jbang-catalog.json"
      ],
      "matchStrings": [
        "https:\\/\\/github\\.com\\/(?<depName>.*)\\/releases\\/download\\/(?<currentValue>.*)\\/.*\\.jar"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
